{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "eslint-plugin-import": "^2.29.1",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "no-useless-escape": "off",
    "no-unused-vars": "warn"
  }
}
